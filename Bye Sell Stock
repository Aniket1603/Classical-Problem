package arrays;
import java.util.*;
public class Strockbuysell {
    public static void main(String[] args) {
        int prices[]  = {7,1,5,3,6,};
        int n= prices.length;
        int buystock = Integer.MAX_VALUE;
        int profit = 0;
        int maxprofit=0;
        for(int i=0;i<n;i++)
        {
            if(buystock < prices[i])
            {
                profit = prices[i] - buystock; 
                if (maxprofit < profit)
                {
                    maxprofit = profit;
                }
            }else{
                
                buystock = prices[i];
            }
            
        }
        System.out.println(maxprofit);
    }
    
}
